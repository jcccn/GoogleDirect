{
  "name": "谷歌极速搜索",
  "version": "0.1",
  "description": "减小被墙的烦恼：直接打开Google搜索结果中的链接，跳过Google的重定向。",
  "browser_action": {
    "default_icon": "images/icon_24.png"
  },
  "icons": {
      "128": "images/icon_128.png"
   },
  "options_page": "options.html",
  "background_page": "background.html",
  "permissions": [
		"contextMenus",
        "tabs",
        "http://*.google.*",
		"https://*.google.*"
    ]
}