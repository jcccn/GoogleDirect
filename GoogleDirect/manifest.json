{
  "name": "谷歌极速搜索",
  "version": "0.2",
  "manifest_version": 2,
  "description": "减小被墙的烦恼：右键直接打开Google搜索结果中的链接，跳过Google的重定向。",
  "browser_action": {
    "default_icon": "images/icon_24.png"
  },
  "icons": {
      "128": "images/icon_128.png"
   },
  "options_page": "options.html",
  "background": {
    "page": "background.html"
    },
  "permissions": [
		"contextMenus",
    "tabs",
    "http://*.google.com/",
		"https://*.google.com/",
    "http://*.google.com.hk/",
    "https://*.google.com.hk/"
    ]
}